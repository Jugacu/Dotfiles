[user]
    # Change your git user here.
	email = julian@cheerfy.com
	name = julian

[core]
    editor = nvim

[init]
    defaultBranch = main

[core]
    pager = delta

[interactive]
    diffFilter = delta --color-only

[delta]
    navigate = true

[push]
	autoSetupRemote = true

[pull]
	rebase = false

[diff]
    colorMoved = default
